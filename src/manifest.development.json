{
    "manifest_version": 3,
    "name": "Froggo",
    "description": "Pull Requests que transmiten cultura",
    "permissions": [
        "clipboardWrite"
    ],
    "host_permissions": [
        "https://froggo.platan.us/*"
    ],
    "version": "0.2.0",
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "action": {
        "default_popup": "popup.html"
    }
}